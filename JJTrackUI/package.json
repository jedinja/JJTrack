{
	"name": "jjtrackui",
	"version": "1.0.0",
	"description": "JJTrack app",
	"main": "app.js",
	"scripts": {
		"deploy": "npm install && webpack --progress --colors --config webpack.prod.js --mode production",
		"deploy-debug": "webpack --progress --colors --config webpack.dev.js --mode development",
